{
  "version": 2,
  "builds": [
    {
      "src": "api/ping.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/ping",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/ping.js"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ]
}
